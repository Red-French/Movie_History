<!-- PUT OMDB MOVIE POSTER AND MOVIE INFO HERE -->
<div class="container">
  <div class="row">
    {{#each Search}} 
      <div class="col-md-3">
        <div class="OMDBmovie">
          
            <img class="movie-poster" src="{{Poster}}" alt="{{Title}}" imdbID="{{imdbID}}" data-toggle="modal" data-target="#movie-modal">
          
        </div>
        <button class="btn-add-movie" type="button" imdbID="{{imdbID}}">Add</button>
      </div>
    {{/each}}
  </div>
</div>