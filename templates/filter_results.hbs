<!-- PUT OMDB MOVIE POSTER AND MOVIE INFO HERE -->
<div class="container">
  <div class="row">
    {{#each this}} 
      <div class="col-md-3">
        <div class="movie">
          <img class="movie-poster" src="{{Poster}}" alt="{{Title}}">
          <button id="btn-delete-movie" class="glyphicon glyphicon-remove-circle"></button>
        </div>

        {{#if Rating}}
          <button class="btn-watched-movie" type="button">Watched</button>

          {{else}}
            <div class="starz">
              {{#times Stars}}
                <img src='data:image/svg+xml;base64,'>
              {{/times}}
            </div>
        {{/if}}
      </div>
    {{/each}}
  </div>
</div>